  "bugs": {
    "url": "https://github.com/PatrykBaranek/controller-chronicles/issues"
  },
  "homepage": "https://github.com/PatrykBaranek/controller-chronicles#readme",
  "devDependencies": {
    "concurrently": "^8.0.1"
  }
}
