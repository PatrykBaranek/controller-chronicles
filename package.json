{
  "name": "controller-chronicles",
  "version": "1.0.0",
  "scripts": {
    "start:client": "npm run dev --prefix ./client",
    "start:server": "npm run start:dev --prefix ./server",
    "installDev": "concurrently \"cd client && npm install\" \"cd server && npm install\"",
    "start": "concurrently \"npm run start:client\" \"npm run start:server\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PatrykBaranek/controller-chronicles.git"
  },
